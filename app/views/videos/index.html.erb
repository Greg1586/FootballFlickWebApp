<div class="container">
    <div id="header" align="center"><h1>Football Flick</h1></div>

    <div id="videos" class="container">
        <% @videos.each do |video| %>
            <div class="five columns" align="center">

              <div class="row thumbnail-photo">
                <%= link_to video.title,video_path(video) %>

                  <%= link_to image_tag(video.icon) ,video_path(video) %>

              </div>
            </div>
        <%end%>
    </div>

    <div id="footer">
        <br/>footer
    </div>
</div>
<div id="buttons" class="footer">
  <form>
    <button id="vow" class="override-button">Video of the week</button>
    <button id="jv" class="override-button">John's video</button>
  </form>
</div>